<div id="webcanvas"></div>
<form action='/jamboard?_id=<%= jambaord_id %>&_uid=<%= userid %>' method="POST" id="adduser">
    <select name="add">
        <% users.forEach(user=>{ %>
            <option value="<%= user.email %>"><%= user.email %></option>
        <% }) %> 
        <% users2.forEach(user2=>{ %>
            <option value="<%= user2.mail %>"><%= user2.mail %></option>
        <% }) %> 
    </select>
    <button type="submit">Add</button>
</form>


<div style="text-align: center;">
    <button id="btnerase">Clear</button>
</div>